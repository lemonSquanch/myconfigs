# Aliases
alias gs="git status";
alias gd="git diff";
alias gk="gitk";

alias repo="repo --color=always --time";

alias df="df -h";

alias ll="ls -lah";
alias ls="ls --color=auto";
alias grep="grep --color";

# Variables
# Note: Set PATH variable value before sourceing this file.
export SQCHY_CLANG_FORMAT_PY="$SQCHY_CLANG_FORMAT_PY_PATH";
